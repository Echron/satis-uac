{
  "name": "echron/packagist-uac",
  "description": "Set up a private packagist with user access control",
  "require": {
    "php": "^8.1",
    "composer/satis": "dev-main",
    "ext-json": "*"
  },
  "require-dev": {
    "monolog/monolog": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "Echron\\Satis\\UAC\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/satis": true
    }
  }
}
